{
  "name": "blueways/bw-email",
  "description": "TYPO3 extension for creating responsive email templates",
  "type": "typo3-cms-extension",
  "require": {
    "tijsverkoyen/css-to-inline-styles": "^2.2",
    "maikschneider/inky": "1.3.6.*",
    "ext-json": "*"
  },
  "require-dev": {
    "typo3/cms-base-distribution": "^11.5",
    "bk2k/bootstrap-package": "dev-master",
    "ssch/typo3-rector": "^1.2"
  },
  "license": "MIT",
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "public",
      "extension-key": "bw_email"
    }
  },
  "authors": [
    {
      "name": "Maik Schneider",
      "email": "m.schneider@blueways.de",
      "homepage": "http://blueways.de",
      "role": "Developer"
    }
  ],
  "keywords": [
    "TYPO3",
    "Zurb Foundation",
    "Responsive Email"
  ],
  "autoload": {
    "psr-4": {
      "Blueways\\BwEmail\\": "Classes"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true,
      "helhum/typo3-console-plugin": true
    }
  }
}
