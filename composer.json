{
  "name": "blueways/bw-email",
  "description": "TYPO3 extension for creating responsive email templates",
  "type": "typo3-cms-extension",
  "require": {
    "tijsverkoyen/css-to-inline-styles": "^2.2",
    "maikschneider/inky": "1.3.6.*",
    "ext-json": "*",
    "ddeboer/imap": "^1.10"
  },
  "require-dev": {
    "typo3/cms-base-distribution": "9.5.*",
    "bk2k/bootstrap-package": "9.1.*",
    "typo3/cms-styleguide": "^10.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Maik Schneider",
      "email": "m.schneider@blueways.de",
      "homepage": "http://blueways.de",
      "role": "Developer"
    }
  ],
  "config": {
    "bin-dir": "bin",
    "vendor-dir": "vendor",
    "platform": {
      "php": "7.4"
    }
  },
  "keywords": [
    "TYPO3",
    "Zurb Foundation",
    "Responsive Email"
  ],
  "autoload": {
    "psr-4": {
      "Blueways\\BwEmail\\": "Classes"
    }
  }
}
